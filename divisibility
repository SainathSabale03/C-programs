#include<iostream>
using namespace std;

int main(){
  int num1;

  cout<<"Enter number1 :";
  cin>>num1;
  cout<<endl;
  // cout<<"Enter number2 :";
  // cin>>num2;

  if(num1 % 5 == 0 && num1 % 3 == 0){
    cout<<"Number is divisible by 5 and 3";
  }
  else{
    cout<<"Number is not divisible by 5 and 3";
  }
}
